{
    "version": 1,
    "request": {
        "url": "https://api.coingecko.com/api/v3/derivatives/exchanges",
        "method": "GET",
        "params": {
            "order": true,
            "perpage": false,
            "page": false
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.coins.[*].items",
        "schema": {
            "id": {
                "target": "$.id",
                "type": "string"
            }
        },
        "orient": "records"
    }
}